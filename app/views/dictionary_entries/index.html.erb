<h1>Listing dictionary_entries</h1>

<table>
  <tr>
    <th>User</th>
    <th>Word</th>
    <th>Definition</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @dictionary_entries.each do |dictionary_entry| %>
  <tr>
    <td><%= dictionary_entry.user_id %></td>
    <td><%= dictionary_entry.word %></td>
    <td><%= dictionary_entry.definition %></td>
    <td><%= link_to 'Show', dictionary_entry %></td>
    <td><%= link_to 'Edit', edit_dictionary_entry_path(dictionary_entry) %></td>
    <td><%= link_to 'Destroy', dictionary_entry, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Dictionary entry', new_dictionary_entry_path %>
